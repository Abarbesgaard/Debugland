{
  "metadata": [
    {
      "src": [
        {
          "src": "../Debugland",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/Tests/**", // Exclude all files in the 'Tests' folder
            "**/*Test.cs",  // Exclude all files ending with 'Test.cs'
            "**/*Tests.cs"  // Exclude all files ending with 'Tests.cs'
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    
    "content": [
      
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Debugland",
      "_appTitle": "Debugland",
      "_enableSearch": true,
      "pdf": true,
      "_appLogoPath": "icon.svg"
      
    }
  }
}
